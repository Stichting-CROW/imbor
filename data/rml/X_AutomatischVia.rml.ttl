:X_AutomatischVia
    a rr:TriplesMap ;

    rml:logicalSource [
        rml:source "data/externe-bronnen/X/X_AutomatischVia.csv" ;
        rml:referenceFormulation ql:CSV ; 
    ] ;

    rr:subjectMap [
        rr:template "http://linkeddata.crow.nl/imbor/def/objecttype/BRON{AutomatischViaID}" ;
        rr:graph graph:X_AutomatischVia ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdfs:label ;
        rr:objectMap [
            rml:reference "AutomatischVia" ;
        ] ;
    ] ;
.