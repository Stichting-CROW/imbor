:REF_TypePlus2
    a rr:TriplesMap ;

    rml:logicalSource [
        rml:source [
            a csvw:Table ;
            csvw:url "data/externe-bronnen/REF/REF_TypePlus2.csv" ;
            csvw:dialect [
                a csvw:Dialect ;
                csvw:encoding "Windows-1252" ;
            ]
        ] ;
        rml:referenceFormulation ql:CSV ; 
    ] ;

    rr:subjectMap [
        rr:template "http://linkeddata.crow.nl/imbor/id/objecttype/OBE{TypePlus2ID}" ;
        rr:class rdfs:Class ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate imborp:guid ;
        rr:objectMap [
            rr:template "urn:uuid:{TypePlus2GUID}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate imborp:typeplus ;
        rr:objectMap [
            rr:template "http://linkeddata.crow.nl/imbor/id/objecttype/OBD{TypePlusID}";
            rr:termType rr:IRI ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdfs:label ;
        rr:objectMap [
            rml:reference "TypePlus2" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate skos:altLabel ;
        rr:objectMap [
            rml:reference "Synoniemen" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate skos:definition ;
        rr:objectMap [
            rml:reference "Definitie" ;
        ] ;
    ] ;
.

