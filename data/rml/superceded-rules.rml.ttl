:X_ObjecttypesAttributen:
    a rr:TriplesMap ;
    
    rml:logicalSource [
        rml:source [
            a csvw:Table ;
            csvw:url "data/externe-bronnen/X_ObjecttypesAttributen.csv" ;
            csvw:dialect [
                a csvw:Dialect ;
                csvw:encoding "Windows-1252" ; # bizar dit
            ]
        ] ;
        rml:referenceFormulation ql:CSV ; 
    ] ;

    rr:subjectMap [
        rr:template "http://linkeddata.crow.nl/imbor/id/groepering/OA{ObjecttypeAttribuutID}" ;
        rr:class imbor:ObjecttypeAttribuutToewijzing ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate imborp:guid ;
        rr:objectMap [
            rr:template "urn:uuid:{ObjecttypeAttribuutGUID}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate imborp:object ;
        rr:objectMap [
            rr:template "http://linkeddata.crow.nl/imbor/id/objecttype/OBB{ObjecttypeID}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate imborp:attribuut ;
        rr:objectMap [
            rr:template "http://linkeddata.crow.nl/imbor/id/eigenschap/P{AttribuutID}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate imborp:relatie ;
        rr:objectMap [
            rr:template "http://linkeddata.crow.nl/imbor/id/objecttype/RELTYPE{RelatieID}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate skos:editorialNote ;
        rr:objectMap [
            rml:reference "ObjecttypeAttribuutToelichting" ;
        ] ;
    ] ;

    rr:predicateObjectMap :informatiemodel ;
.

