    ] ;

    rr:predicateObjectMap [
        rr:predicate skos:definition ;
        rr:objectMap [
            rml:reference "ObjecttypeGroepToelichting" ;
        ] ;
    ] ;

    rr:predicateObjectMap :txtFormuliernaam ;
.


