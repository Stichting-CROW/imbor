:X_ObjecttypeGroepen
    a rr:TriplesMap ;
    rml:logicalSource [
        rml:source "data/externe-bronnen/X_ObjecttypeGroepen.csv" ;
        rml:referenceFormulation ql:CSV ;
        rml:encoding "utf-8" ;
    ] ;
    
    rr:subjectMap [
        rr:template "http://linkeddata.crow.nl/imbor/id/objecttype/OBA{ObjecttypeGroepID}" ;
        rr:class rdfs:Class ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate imborp:guid ;
        rr:objectMap [
            rr:template "urn:uuid:{ObjecttypeGroepGUID}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;

    rr:predicateObjectMap :informatiemodel ;

    rr:predicateObjectMap [
        rr:predicate rdfs:label ;
        rr:objectMap [
            rml:reference "ObjecttypeGroep" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
