# Ik krijg geen xsd:boolean waardes uit CSVW / RML
# Daarom deze handmatige vervanging

DELETE {
    ?s ?p ?label .
}

INSERT {
    ?s ?p true .
}

WHERE {
    ?s ?p ?label .
    FILTER ( str(?label) = "True" )
};

DELETE {
    ?s ?p ?label .
}

INSERT {
    ?s ?p false .
}

WHERE {
    ?s ?p ?label .
    FILTER ( str(?label) = "False" )
}
