{
  "$schema": "https://rdmr.eu/ns/sparql-query-runner/schema.json",
  "pipeline": {
    "name": "IMBOR 2021",
    "endpoint": "http://localhost:7200/repositories/imbor-2021/statements",
    "prefixes": {
      "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
      "owl": "http://www.w3.org/2002/07/owl#",
      "imbor-domeinwaarde": "https://data.crow.nl/imbor/id/domeinwaarden/",
      "imbor": "https://data.crow.nl/imbor/def/",
      "imbor-term": "https://data.crow.nl/imbor/term/",
      "imbor-refmodels": "https://data.crow.nl/imbor-ref/def/",
      "imbor-meta": "https://data.crow.nl/imbor/aanvullend-metamodel/",
      "quantitykind": "http://qudt.org/vocab/quantitykind/",
      "nen2660": "https://w3id.org/nen2660/def#",
      "nen2660-term": "https://w3id.org/nen2660/term#",
      "nen3610": "http://definities.geostandaarden.nl/def/nen3610#",
      "mim": "http://bp4mc2.org/def/mim#",
      "unit": "http://qudt.org/vocab/unit/",
      "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
      "xsd": "http://www.w3.org/2001/XMLSchema#",
      "dct": "http://purl.org/dc/terms/",
      "dc": "http://purl.org/dc/elements/1.1/",
      "geo": "http://www.opengis.net/ont/geosparql#",
      "sh": "http://www.w3.org/ns/shacl#",
      "skos": "http://www.w3.org/2004/02/skos/core#",
      "dash": "http://datashapes.org/dash#"
    },
    "steps": [
      {
        "type": "sparql",
        "url": "src/transformations/clear-all.rq"
      },
      {
        "type": "set-prefixes",
        "url": "http://localhost:7200/repositories/imbor-2021"
      },
      {
        "type": "import-msaccess",
        "url": "data/IMBOR-2022.c08.accdb"
      },
      {
        "type": "delay"
      },
      {
        "type": "sparql",
        "url": [
          "src/transformations/addendum-geometrie.rq",
          "src/transformations/attributen.rq",
          "src/transformations/attribuutshapes.rq",
          "src/transformations/enumeratietypes-domeinwaarden.rq",
          "src/transformations/enumeratietypes-gesloten.rq",
          "src/transformations/enumeratietypes-klasses.rq",
          "src/transformations/imbor-metaontologie.rq",
          "src/transformations/informatiemodellen.rq",
          "src/transformations/klassen-definities.rq",
          "src/transformations/klassen-hierarchie.rq",
          "src/transformations/minimale-dataset.rq",
          "src/transformations/oagbd.rq",
          "src/transformations/referentiemodel-attributen.rq",
          "src/transformations/referentiemodel-domeinwaarden.rq",
          "src/transformations/referentiemodel-mapping.rq",
          "src/transformations/referentiemodel-mapping-delete.rq",
          "src/transformations/referentiemodel-mapping-delete-ref.rq",
          "src/transformations/referentiemodel-mapping-delete-all.rq",
          "src/transformations/referentiemodel-objecttypen.rq",
          "src/transformations/semantische-relaties.rq",
          "src/transformations/termen-en-collecties.rq",
          "src/transformations/klassen-hierarchie-skos.rq",
          "src/transformations/unit-quantity-kind.rq",
          "src/transformations/vakdisciplines.rq",
          "src/transformations/owl-ontologies.rq",
          "src/transformations/scheme-skos.rq"
        ]
      },
      {
        "type": "delay"
      },
      {
        "type": "sparql",
        "url": [
          "src/transformations/hernoemen-refurls.rq",
          "src/transformations/hernoemen-refurls.rq",
          "src/transformations/hernoemen-refurls.rq",
          "src/transformations/vocab-hierarchie-1.rq",
          "src/transformations/vocab-hierarchie-2.rq",
          "src/transformations/vocab-hierarchie-3.rq"
        ]
      },
      {
        "type": "sparql",
        "url": [
          "src/transformations/mim-klasses.rq",
          "src/transformations/mim-attributen.rq",
          "src/transformations/mim-relaties.rq",
          "src/transformations/mim-enumeraties.rq",
          "src/transformations/mim-domeinwaarden.rq"
        ]
      },
      {
        "type": "set-prefixes",
        "url": "http://localhost:7200/repositories/imbor-2021"
      },
      {
        "type": "download-file",
        "url": "data/imbor-vocabulair.ttl",
        "graphs": [
          "https://data.crow.nl/imbor/term/"
        ]
      },
      {
        "type": "download-file",
        "url": "data/imbor-ontologie.ttl",
        "graphs": [
          "https://data.crow.nl/imbor/def/"
        ]
      },
      {
        "type": "download-file",
        "url": "data/imbor-domeinwaarden.ttl",
        "graphs": [
          "https://data.crow.nl/imbor/id/domeinwaarden/"
        ]
      },
      {
        "type": "download-file",
        "url": "data/imbor-aanvullend-metamodel.ttl",
        "graphs": [
          "https://data.crow.nl/imbor/aanvullend-metamodel"
        ]
      },
      {
        "type": "download-file",
        "url": "data/imbor-informatief.trig",
        "graphs": [
          "https://data.crow.nl/imbor/addendum/oagbd",
          "https://data.crow.nl/imbor/addendum/geometrie",
          "https://data.crow.nl/imbor/addendum/minimale-dataset",
          "https://data.crow.nl/imbor/addendum/referentiemodellen",
          "https://data.crow.nl/imbor/addendum/mim"
        ]
      },
      {
        "type": "download-file",
        "url": "data/imbor-volledig-onto-verkenner.ttl",
        "graphs": [
          "https://data.crow.nl/imbor/term/",
          "https://data.crow.nl/imbor/def/",
          "https://data.crow.nl/imbor/id/domeinwaarden/",
          "https://data.crow.nl/imbor/aanvullend-metamodel",
          "https://data.crow.nl/imbor/addendum/oagbd",
          "https://data.crow.nl/imbor/addendum/geometrie",
          "https://data.crow.nl/imbor/addendum/minimale-dataset",
          "https://data.crow.nl/imbor/addendum/referentiemodellen",
          "https://data.crow.nl/imbor/addendum/mim"
        ]}
      ]
    }
  }