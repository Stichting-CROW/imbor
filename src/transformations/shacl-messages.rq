INSERT { 
    graph <https://data.crow.nl/imbor/def/> {
        ?s  sh:message "Het attribuut geometrie is gelijk aan de waarde van nen2660:hasBoundary. Wanneer deze gevuld is met een geo:Geometry mag daarmee het attribuut 'Geometrie' leeg zijn." ;
            sh:severity sh:Warning .
    }
}

WHERE {
    graph <https://data.crow.nl/imbor/def/> {
        BIND(imbor:70fa1e34-7061-4de3-940f-f51e0d3f1fa5 AS ?s) #geometrie
        ?s a sh:PropertyShape .
    }
}